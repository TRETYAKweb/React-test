{
    "parser": "@babel/eslint-parser",
    "extends": ["@sotream/eslint-config-react-js"],
    "rules": {
        "no-void": "off",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "jsx-quotes": ["error", "prefer-single"],
        "react/jsx-equals-spacing": "off"
    }
}
